    "def esPrimo(n):\n",
    "    if n <=1:\n",
    "        return False\n",
    "    elif n ==2:\n",
    "        return True\n",
    "    else:\n",
    "        for i in range(2, n):\n",
    "            if n % i == 0:\n",
    "                return False\n",
    "        return True\n",
    "    \n",
    "for i in range(-10, 101):\n",
    "    print(i, \" \", esPrimo(i)) "
